{
	"extends": "@istanbuljs/nyc-config-typescript",
	"all": true,
	"include": ["src/frontend/src/**/*.ts", "src/frontend/src/**/*.svelte"],
	"exclude": [
		"**/*.d.ts",
		"**/*.test.ts",
		"**/*.spec.ts",
		"**/index.ts",
		"**/types.ts",
		"src/frontend/src/test/**"
	],
	"reporter": ["text", "html", "lcov", "json"],
	"check-coverage": true,
	"branches": 80,
	"lines": 80,
	"functions": 80,
	"statements": 80
}
